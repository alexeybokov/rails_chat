<%= link_to 'Chatroom', root_path, class: 'item' %>
<a class="item">
  Messages
</a>
<div class="right menu">
  <div class="ui dropdown item">
    Account <i class="dropdown icon"></i>
    <div class="menu">
      <% if current_user %>
        <%#= link_to 'Log Out', logout_path, method: :delete, class: 'item' %>
        <%= link_to 'Log Out', destroy_user_session_path, method: :delete, class: 'item' %>
      <% else %>
        <%#= link_to 'Log In', login_path, class: 'item' %>
        <%= link_to 'Log In', new_user_session_path, class: 'item' %>
      <% end %>
    </div>
  </div>
  <% unless current_user %>
    <div class="item">
      <%#= link_to 'Log In', login_path, class: 'ui primary button' %>
      <%= link_to 'Log In', new_user_session_path, class: 'ui primary button' %>
    </div>
  <% end %>
</div>
